generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Player {
  id            Int      @id @default(autoincrement())
  name          String
  age           String     
  number        Int
  position      String
  birthday      DateTime
  created_at    DateTime @default(now())
  update_at     DateTime @updatedAt
}